{
  "name": "othercode/rest",
  "type": "library",
  "license": "MIT",
  "description": "Rest client to make GET, POST, PUT, DELETE, PATCH, etc calls.",
  "keywords": [
    "http",
    "curl",
    "rest",
    "requests"
  ],
  "authors": [
    {
      "name": "Unay Santisteban",
      "email": "usantisteban@othercode.es",
      "homepage": "http://othercode.es/",
      "role": "developer"
    }
  ],
  "require": {
    "php": ">=7.4.33",
    "ext-curl": "*",
    "ext-xmlrpc": "*",
    "ext-simplexml": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "pestphp/pest": "^1.0",
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "OtherCode\\Rest\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "pest"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
